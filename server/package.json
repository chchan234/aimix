    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.13",
    "drizzle-kit": "^0.31.7",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
