    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^4.77.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
